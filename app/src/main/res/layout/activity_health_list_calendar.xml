<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_health"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="三检列表日历" />

    <com.szy.szycalendar.CalendarView
        android:id="@+id/calendarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:szyCalendarExEnable="true"
        app:szyMonthBarPath="com.szy.szycalendar.month.DefaultMonthBar"
        app:szyDateViewPath="com.szy.szycalendar.date.HealthListDateView"
        app:szyMonthTitleClickEnable="false" />

</LinearLayout>
